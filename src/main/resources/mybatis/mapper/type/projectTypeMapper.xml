<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.alvinsite.demo.dao.type.ProjectTypeDao">
    <select id="findAll" resultType="top.alvinsite.demo.model.dto.type.ProjectTypeDTO">
        select
            bzm as id,
            flmc as title
        from gxky0117
        order by ID ASC
    </select>

    <select id="findOne"  resultType="top.alvinsite.demo.model.dto.type.ProjectTypeDTO">
        select
            bzm as id,
            flmc as title
        from s_unit
        where ID = #{id}
    </select>
</mapper>